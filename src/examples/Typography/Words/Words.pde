/**
 * Words.
 *
 * The text() function is used for writing words to the screen.
 */

void setup() {
  background(102);
}

void loop() {
  // Use stroke() to change the value or color of the text
  stroke(0);
  text("ichi", 30, 60);
  stroke(51);
  text("ni", 30, 95);
  stroke(204);
  text("san", 30, 130);
  stroke(255);
  text("shi", 30, 165);
}
